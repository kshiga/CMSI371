/*
 *Problem 25c. A 50% translucent red rectangle overlapping a 50% translucent green rectangle
 *Assignment 0129 
 *CMSI371 
 *Spring 2013
 */

(function () {
    var canvas = document.getElementById("canvas"),

        lines = canvas.getContext("2d"),
        edgeRight = (canvas.width - 1),
        edgeBottom = (canvas.height - 1),

        lines.beginPath(); 
        lines.lineTo(edgeRight, edgeBottom);

        lines.beginPath();
        lines.moveTo(0, edgeRight);
        lines.lineTo(edgeBottom, 0);
}());
